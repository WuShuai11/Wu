/**my.wxss**/

page {
  min-height: 100%;
  background: #f7f7f7;
}

.head {
  width: 690rpx;
  height: 200rpx;
  background: #fff;
  border-radius: 6rpx;
  margin: 16rpx auto;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;
}

.head image {
  display: inline-block;
  width: 112rpx;
  height: 112rpx;
  border-radius: 50%;
  margin: 44rpx 1rpx 30rpx 30rpx;
  vertical-align: top;
  margin-left: -30rpx;
}

.head view {
  display: inline-block;
  width: 100rpx;
  /* left: 10rpx; */
  height: 40rpx;
  line-height: 40rpx;
  font-size: 40rpx;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
  margin: 80rpx 0;
  /* vertical-align: top; */
}

.r {
  float: right;
  width: 120rpx;
  height: 48rpx;
  background: rgba(244, 199, 156, 1);
  border-radius: 28rpx;
  line-height: 48rpx;
  text-align: center;
  margin: 75rpx 15rpx 0 0;
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.wode_image2 {
  width: 28rpx;
  height: 27rpx;
  background: rgba(255, 255, 255, 1);
}

.order {
  width: 686rpx;
  padding: 20rpx 0;
  background: #fff;
  margin: 0 auto 16rpx;
  border-radius: 6rpx;
}

.order .title {
  height: 42rpx;
  font-size: 30rpx;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
  line-height: 42rpx;
  margin: 0 20rpx 32rpx;
}

.order .order_box {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 12rpx;
}

.order .order_box .box {
  width: 25%;
}

.order .order_box .box image {
  height: 40rpx;
  width: 100%;
}

.order .order_box .box view {
  margin-top: 8rpx;
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
  line-height: 36rpx;
  text-align: center;
}

.option {
  width: 686rpx;
  padding: 13rpx 0 5rpx;
  background: #fff;
  margin: 0 auto;
  border-radius: 6rpx;
}

.option .option_box {
  height: 80rpx;
  line-height: 80rpx;
}

.option .option_box .icon {
  float: left;
  width: 40rpx;
  height: 40rpx;
  margin: 18rpx 26rpx 0 36rpx;
  vertical-align: top;
}

.option .option_box .text {
  float: left;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
}

.option .option_box .angle {
  width: 18rpx;
  height: 30rpx;
  float: right;
  margin: 25rpx 40rpx 0 0;
  font-size: 35rpx;
  color: #c7c7cc;
  vertical-align: top;
}
